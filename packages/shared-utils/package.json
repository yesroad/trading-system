  "dependencies": {
    "dotenv": "^17.2.4",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.4.2",
    "@workspace/eslint-config": "*",
    "eslint": "^9.39.2",
    "typescript": "^5.9.2"
