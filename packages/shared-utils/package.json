{
  "name": "@workspace/shared-utils",
  "version": "1.0.0",
  "packageManager": "yarn@4.9.2",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
